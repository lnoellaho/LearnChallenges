<h2><%= todoList.get('name') %></h2>
<div>
  <h3>New Todo</h3>
  <form
    action="/todo-list/<%= todoList.get('id') %>/todo/new"
    method="post"
  >
    <input type='text' name='name' />
    <input type='submit' value='Add Todo' />
  </form>
</div>

<ol>
  <% for(index in todos){ %>
    <li>
      <% if(!todos[index].isComplete){ %>
        <form
          action="/todo-list/<%= todoList.get('id') %>/todo/<%= todos[index].get('id') %>/complete"
          method="post"
        >
          <%= todos[index].name %>
          <input type='submit' value='complete' />
      </form>
      <% }else{ %>
        <span class='complete' >
          <%= todos[index].name %> <br / />
          <form
            action="/todo-list/<%= todoList.get('id') %>/todo/<%= todos[index].get('id') %>/incomplete"
            method="post"
          >
            <input type='submit' value='incomplete' />
        </form>
        <form
          action="/todo-list/<%= todoList.get('id') %>/todo/<%= todos[index].get('id') %>/delete"
          method="post"
        >
          <span class='complete' >
          </span>
          <input type='submit' value='remove' />
        </form>
        </span>
      <% } %>
    </li>
  <% } %>
</ol>
